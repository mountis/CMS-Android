<android.support.design.widget.CoordinatorLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:name="com.augimas.android.fragments.BrandingElementFragment"
    tools:context="com.augimas.android.fragments.BrandingElementFragment">

    <include android:id="@+id/no_content"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"
        layout="@layout/layout_no_content_general"/>

    <android.support.v7.widget.LinearLayoutCompat
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:visibility="gone"
        android:id="@+id/branding_element_domain_name_layout"
        android:layout_height="match_parent">
        <android.support.v7.widget.RecyclerView
            android:layout_gravity="top"
            android:id="@+id/domain_name_recycler_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layoutManager="LinearLayoutManager"
            tools:listitem="@layout/list_item_domain_name_single"
            android:layout_marginBottom="65dp"/>
    </android.support.v7.widget.LinearLayoutCompat>
    <android.support.v7.widget.LinearLayoutCompat
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:visibility="gone"
        android:id="@+id/branding_element_social_media_name_layout"
        android:layout_height="match_parent">
        <android.support.v7.widget.RecyclerView
            android:layout_gravity="top"
            android:id="@+id/social_media_name_recycler_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layoutManager="LinearLayoutManager"
            tools:listitem="@layout/list_item_social_media_name_single"
            android:layout_marginBottom="65dp"/>
    </android.support.v7.widget.LinearLayoutCompat>
    <android.support.v7.widget.LinearLayoutCompat
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:visibility="gone"
        android:id="@+id/branding_element_mission_statement_layout"
        android:layout_height="match_parent">
        <android.support.v7.widget.RecyclerView
            android:layout_gravity="top"
            android:id="@+id/mission_statement_recycler_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layoutManager="LinearLayoutManager"
            tools:listitem="@layout/list_item_mission_statements_single"
            android:layout_marginBottom="65dp"/>
    </android.support.v7.widget.LinearLayoutCompat>
    <android.support.v7.widget.LinearLayoutCompat
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:visibility="gone"
        android:id="@+id/branding_element_target_audience_layout"
        android:layout_height="match_parent">
        <android.support.v7.widget.RecyclerView
            android:layout_gravity="top"
            android:id="@+id/target_audience_recycler_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layoutManager="LinearLayoutManager"
            tools:listitem="@layout/list_item_target_audience_single"
            android:layout_marginBottom="65dp"/>
    </android.support.v7.widget.LinearLayoutCompat>
    <android.support.v7.widget.LinearLayoutCompat
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:visibility="gone"
        android:id="@+id/branding_element_logo_layout"
        android:layout_height="match_parent">
        <android.support.v7.widget.RecyclerView
            android:layout_gravity="top"
            android:id="@+id/logo_recycler_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layoutManager="LinearLayoutManager"
            tools:listitem="@layout/list_item_logo_single"
            android:layout_marginBottom="65dp"/>
    </android.support.v7.widget.LinearLayoutCompat>
    <android.support.v7.widget.LinearLayoutCompat
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:visibility="gone"
        android:id="@+id/branding_element_team_members_layout"
        android:layout_height="match_parent">
        <android.support.v7.widget.RecyclerView
            android:layout_gravity="top"
            android:id="@+id/team_members_recycler_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layoutManager="LinearLayoutManager"
            tools:listitem="@layout/list_item_team_members_single"
            android:layout_marginBottom="65dp"/>
    </android.support.v7.widget.LinearLayoutCompat>
    <android.support.v7.widget.LinearLayoutCompat
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:visibility="gone"
        android:id="@+id/branding_element_brand_style_layout"
        android:layout_height="match_parent">
        <android.support.v7.widget.RecyclerView
            android:layout_gravity="top"
            android:id="@+id/brand_style_recycler_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layoutManager="LinearLayoutManager"
            tools:listitem="@layout/list_item_brand_style_single"
            android:layout_marginBottom="65dp"/>
    </android.support.v7.widget.LinearLayoutCompat>
    <android.support.v7.widget.LinearLayoutCompat
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:visibility="gone"
        android:id="@+id/branding_element_product_service_layout"
        android:layout_height="match_parent">
        <android.support.v7.widget.RecyclerView
            android:layout_gravity="top"
            android:id="@+id/product_service_recycler_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layoutManager="LinearLayoutManager"
            tools:listitem="@layout/list_item_product_service_single"
            android:layout_marginBottom="65dp"/>
    </android.support.v7.widget.LinearLayoutCompat>
</android.support.design.widget.CoordinatorLayout>